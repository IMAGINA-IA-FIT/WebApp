{
  "name": "fitness-coach-signed-urls",
  "version": "1.0.0",
  "description": "Cloud Function to generate signed URLs for video uploads",
  "main": "index.js",
  "scripts": {
    "start": "functions-framework --target=generateSignedUrl",
    "deploy": "gcloud functions deploy generateSignedUrl --runtime nodejs18 --trigger-http --allow-unauthenticated"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.0.0",
    "@google-cloud/storage": "^7.0.0"
  },
  "engines": {
    "node": "18"
  }
} 